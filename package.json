    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^5.10.2",
    "@typescript-eslint/parser": "^5.10.2",
    "eslint": "^8.7.0",
    "eslint-config-prettier": "^8.3.0",
    "mjml": "^4.11.0",
    "nodemon": "^2.0.15",
    "prettier": "2.5.1",
    "typescript": "^4.5.5"
  }
