{
  "name": "btree-node",
  "version": "1.0.0",
  "author": "Hannes Oberreiter",
  "license": "AGPL-3.0",
  "private": true,
  "keywords": [],
  "main": "./dist/api/app.bootstrap.js",
  "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
  "description": "b.tree backend based on nodejs, express and knex.js, objections.js.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HannesOberreiter/btree_server.git"
  },
  "bugs": {
    "url": "https://github.com/HannesOberreiter/btree_server/issues"
  },
  "homepage": "https://github.com/HannesOberreiter/btree_server#readme",
  "scripts": {
    "serve": "nodemon",
    "build": "tsc",
    "migrate": "knex migrate:latest",
    "mail": "mjml mjml/* -o mails/ --config.beautify true --config.minify true",
    "kickstart": "concurrently -k --restart-after 1000 'npm:dev:build' 'npm:dev:serve' 'npm:test:watch'",
    "dev:init": "ENVIRONMENT=development knex migrate:latest && ENVIRONMENT=development knex seed:run",
    "dev:serve": "ENVIRONMENT=development nodemon --delay 2000ms",
    "dev:build": "tsc -watch",
    "dev:knex": "ENVIRONMENT=development knex",
    "dev:knex-seed": "ENVIRONMENT=staging knex seed:run",
    "test:init": "ENVIRONMENT=test knex migrate:latest && ENVIRONMENT=test knex seed:run",
    "test:seed": "ENVIRONMENT=test knex seed:run",
    "test:e2e": "ENVIRONMENT=test npx mocha ./test/e2e/00-api.e2e.test.js  --R spec --ui bdd --env test",
    "test:watch": "ENVIRONMENT=test npx mocha ./test/e2e/00-api.e2e.test.js  --watch dist test --env test",
    "ci:init": "ENVIRONMENT=ci knex migrate:latest && ENVIRONMENT=ci knex seed:run",
    "ci:e2e": "ENVIRONMENT=ci npx mocha ./test/e2e/00-api.e2e.test.js",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "_moduleAliases": {
    "@": "dist",
    "@api": "dist/api",
    "@cron": "dist/cron",
    "@config": "dist/config",
    "@servers": "dist/servers",
    "@controllers": "dist/api/controllers",
    "@middlewares": "dist/api/middlewares",
    "@models": "dist/api/models",
    "@routes": "dist/api/routes",
    "@services": "dist/api/services",
    "@enums": "dist/api/types/enums",
    "@classes": "dist/api/types/classes",
    "@interfaces": "dist/api/types/interfaces",
    "@utils": "dist/api/utils"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "ajv-formats": "^2.1.1",
    "awilix": "^7.0.3",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "boom": "^7.3.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dayjs": "^1.11.1",
    "dotenv": "^16.0.0",
    "dropbox": "^10.29.0",
    "errorhandler": "^1.5.1",
    "es6-promisify": "^7.0.0",
    "express": "^4.18.1",
    "express-rate-limit": "^6.4.0",
    "express-useragent": "^1.0.15",
    "express-validator": "^6.14.0",
    "helmet": "^5.0.2",
    "hpp": "^0.2.3",
    "http-status": "^1.5.1",
    "ical-generator": "^3.4.2",
    "jsonwebtoken": "^8.5.1",
    "knex": "^2.0.0",
    "lodash": "^4.17.21",
    "mjml": "^4.12.0",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.5",
    "objection": "^3.0.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "rotating-file-stream": "^3.0.3",
    "winston": "^3.7.2"
  },
  "devDependencies": {
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.31",
    "@types/nodemailer": "^6.4.4",
    "@typescript-eslint/eslint-plugin": "^5.22.0",
    "@typescript-eslint/parser": "^5.22.0",
    "chai": "^4.3.6",
    "concurrently": "^7.2.0",
    "eslint": "^8.14.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.0.0",
    "nodemon": "^2.0.16",
    "prettier": "2.6.2",
    "supertest": "^6.2.3",
    "typescript": "^4.6.4"
  }
}
