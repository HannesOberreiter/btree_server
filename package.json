{
  "name": "btree-node",
  "version": "1.0.0",
  "author": "Hannes Oberreiter",
  "license": "AGPL-3.0",
  "private": true,
  "keywords": [],
  "main": "./dist/app.bootstrap.js",
  "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
  "description": "b.tree backend based on nodejs, express and knex.js, objections.js.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HannesOberreiter/btree_server.git"
  },
  "bugs": {
    "url": "https://github.com/HannesOberreiter/btree_server/issues"
  },
  "homepage": "https://github.com/HannesOberreiter/btree_server#readme",
  "scripts": {
    "serve": "nodemon",
    "build": "tsc",
    "migrate": "knex migrate:latest",
    "dev:init": "ENVIRONMENT=development knex migrate:latest && ENVIRONMENT=development knex seed:run",
    "dev:serve": "ENVIRONMENT=development nodemon --delay 2000ms",
    "dev:build": "tsc --watch",
    "dev:knex": "ENVIRONMENT=development knex",
    "test:e2e": "ENVIRONMENT=test npx mocha ./test/e2e/00-api.e2e.test.js  --R spec --ui bdd --env test",
    "test:watch": "ENVIRONMENT=test npx mocha ./test/e2e/00-api.e2e.test.js  --watch dist test --env test",
    "ci:e2e": "ENVIRONMENT=ci npx mocha ./test/e2e/00-api.e2e.test.js",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "dependencies": {
    "@fastify/compress": "^6.4.0",
    "@fastify/cookie": "^8.3.0",
    "@fastify/helmet": "^11.0.0",
    "@fastify/rate-limit": "^8.0.3",
    "@fastify/session": "^10.4.0",
    "ajv-formats": "^2.1.1",
    "archiver": "^5.3.1",
    "connect-redis": "^7.0.0",
    "csv-stringify": "^6.2.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.2",
    "dropbox": "^10.33.0",
    "fastify": "^4.20.0",
    "fastify-type-provider-zod": "^1.1.9",
    "google-auth-library": "^9.0.0",
    "gpt-3-encoder": "^1.1.4",
    "http-errors": "^2.0.0",
    "ical-generator": "^5.0.0",
    "ioredis": "^5.3.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "mysql": "^2.18.1",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.8",
    "objection": "^3.0.1",
    "openai": "^3.2.1",
    "pino": "^8.14.1",
    "pino-abstract-transport": "^1.0.0",
    "pino-pretty": "^10.0.1",
    "rotating-file-stream": "^3.0.4",
    "stripe": "^12.0.0",
    "ua-parser-js": "^1.0.35"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/lodash": "^4.14.191",
    "@types/node": "^20.3.1",
    "@types/node-schedule": "^2.1.0",
    "@types/nodemailer": "^6.4.7",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "chai": "^4.3.6",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "prettier": "^3.0.0",
    "supertest": "^6.3.3",
    "typescript": "^5.0.4"
  }
}
