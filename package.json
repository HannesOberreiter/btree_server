{
  "name": "btree-node",
  "version": "1.0.0",
  "author": "Hannes Oberreiter",
  "license": "AGPL-3.0",
  "private": true,
  "keywords": [],
  "main": "./dist/api/app.bootstrap.js",
  "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
  "description": "b.tree backend based on nodejs, express and knex.js, objections.js.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/HannesOberreiter/btree_server.git"
  },
  "bugs": {
    "url": "https://github.com/HannesOberreiter/btree_server/issues"
  },
  "homepage": "https://github.com/HannesOberreiter/btree_server#readme",
  "scripts": {
    "serve": "nodemon",
    "build": "tsc",
    "migrate": "knex migrate:latest",
    "dev:init": "ENVIRONMENT=development knex migrate:latest && ENVIRONMENT=development knex seed:run",
    "dev:serve": "ENVIRONMENT=development nodemon --delay 2000ms",
    "dev:build": "tsc -watch",
    "dev:knex": "ENVIRONMENT=development knex",
    "test:coverage": "ENVIRONMENT=test nyc --reporter=text mocha ./test/e2e/00-api.e2e.test.js",
    "test:e2e": "ENVIRONMENT=test npx mocha ./test/e2e/00-api.e2e.test.js  --R spec --ui bdd --env test",
    "test:watch": "ENVIRONMENT=test npx mocha ./test/e2e/00-api.e2e.test.js  --watch dist test --env test",
    "ci:e2e": "ENVIRONMENT=ci npx mocha ./test/e2e/00-api.e2e.test.js",
    "prettier-format": "prettier --config .prettierrc 'src/**/*.ts' --write",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx"
  },
  "_moduleAliases": {
    "@": "dist",
    "@api": "dist/api",
    "@cron": "dist/cron",
    "@config": "dist/config",
    "@servers": "dist/servers",
    "@controllers": "dist/api/controllers",
    "@middlewares": "dist/api/middlewares",
    "@models": "dist/api/models",
    "@routes": "dist/api/routes",
    "@services": "dist/api/services",
    "@enums": "dist/api/types/enums",
    "@classes": "dist/api/types/classes",
    "@interfaces": "dist/api/types/interfaces",
    "@utils": "dist/api/utils"
  },
  "dependencies": {
    "@hapi/boom": "^10.0.0",
    "ajv-formats": "^2.1.1",
    "archiver": "^5.3.1",
    "awilix": "^8.0.0",
    "axios": "^1.2.2",
    "body-parser": "^1.20.1",
    "compression": "^1.7.4",
    "connect-redis": "^7.0.0",
    "connect-session-knex": "^3.0.0",
    "cors": "^2.8.5",
    "csv-stringify": "^6.2.3",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.2",
    "dropbox": "^10.33.0",
    "express": "^4.18.2",
    "express-rate-limit": "^6.6.0",
    "express-session": "^1.17.3",
    "express-useragent": "^1.0.15",
    "express-validator": "^6.14.2",
    "form-data": "^4.0.0",
    "gpt-3-encoder": "^1.1.4",
    "helmet": "^6.0.1",
    "hpp": "^0.2.3",
    "ical-generator": "^4.0.0",
    "ioredis": "^5.3.1",
    "knex": "^2.3.0",
    "lodash": "^4.17.21",
    "module-alias": "^2.2.2",
    "morgan": "^1.10.0",
    "mysql": "^2.18.1",
    "node-schedule": "^2.1.0",
    "nodemailer": "^6.7.8",
    "objection": "^3.0.1",
    "openai": "^3.2.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "rotating-file-stream": "^3.0.4",
    "stripe": "^12.0.0",
    "winston": "^3.8.2"
  },
  "devDependencies": {
    "@types/archiver": "^5.3.1",
    "@types/body-parser": "^1.19.2",
    "@types/express": "^4.17.15",
    "@types/express-session": "^1.17.5",
    "@types/lodash": "^4.14.191",
    "@types/node": "^18.11.18",
    "@types/nodemailer": "^6.4.7",
    "@types/passport": "^1.0.12",
    "@types/passport-google-oauth20": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.47.1",
    "@typescript-eslint/parser": "^5.47.1",
    "chai": "^4.3.6",
    "eslint": "^8.30.0",
    "eslint-config-prettier": "^8.5.0",
    "mocha": "^10.2.0",
    "nodemon": "^2.0.20",
    "nyc": "^15.1.0",
    "prettier": "^2.8.1",
    "supertest": "^6.3.3",
    "typescript": "^4.9.4"
  }
}
